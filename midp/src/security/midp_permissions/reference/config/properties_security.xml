<!-- System-independent Security properties required by the specification -->
<configuration>
<properties>
<!-- SYSTEM PROPERTIES -->
<!-- Visible from System.getProperty() -->

  <property Key="security.policyfile"
      Value="_policy.txt"
      Scope="system"
      Comment="policy file"/>

  <property Key="security.messagefile"
      Value="_function_groups.txt"
      Scope="system"
      Comment="policy group messages file"/>

<!-- INTERNAL PROPERTIES -->
<!-- Visible from Configuration.getProperty() -->
</properties>
</configuration>

