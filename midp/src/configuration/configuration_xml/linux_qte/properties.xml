<!-- THIS IS THE PROPERTIES FILE FOR SHARP ZAURUS 5500D ONLY!
	 YOU MAY NEED TO ADJUST THAT TO YOUR PLATFORM -->
<configuration>
<properties>
<!-- INTERNAL PROPERTIES -->
<!-- Visible from Configuration.getProperty() -->
  <!-- System tunable parameters -->

  <!-- This is the command used for platformRequest() to launch URLs -->
  <property Key="com.sun.midp.midlet.platformRequestCommand"
				Value="opera"
				Scope="internal"/>				

  <property Key="system.jam_space"
		Value="1000000"
		Scope="internal"
		Comment="Amount of space to reserve for JAM storage."/>

<!-- SYSTEM PROPERTIES -->
<!-- Visible from System.getProperty() -->
  <property Key="path.separator" 
		Value=":" 
		Scope="system"
		Comment="Path separator."/>
</properties>
</configuration>
